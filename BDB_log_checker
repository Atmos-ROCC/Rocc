for i in $(rmsview -l mauimds | grep $HOSTNAME | sort); do rmgmaster=$(grep localDb /etc/maui/cm_cfg.xml|cut -d ',' -f2); port=$(echo $i|awk -F ':' '{print $3}'); [[ $(mauisvcmgr -s mauimds -c mauimds_isNodeInitialized | grep $port | awk -F'=' '{print $2}') = "true" ]] && continue;master=$(mauimdlsutil -q -m  '*' -n $rmgmaster |grep -B 3 $HOSTNAME:$port|awk '/Master/{print $4}');masterport=$(echo $master|awk -F ':' '{print $2}');masterhost=$(echo $master|awk -F ':' '{print $1}');ls -ltr mdsdir $port |grep ' log.*' |tail -1 |awk -F '.' '{print $2}' > tmp.local;localhlsn=$(cat tmp.local);ssh $masterhost 'ls -ltr mdsdir '$masterport'' |grep ' log.*' |tail -1 |awk -F '.' '{print $2}' > tmp.master;masterhlsn=$(cat tmp.master);logbehind=$((10#$masterhlsn-10#$localhlsn));if [[ $logbehind -ne 0 ]]; then [[ -a /var/tmp/$i ]] && { lastpoll=$(cat /var/tmp/$i) && lastcurrent=$(($logbehind-$lastpoll)); };echo -e "Port: "$port   "incr/decr:" $lastcurrent '\n'"Logs behind: "$logbehind;fi;echo $logbehind > /var/tmp/$i;done;rm -f tmp.master tmp.local 
